#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/Users/david/Documents/Work/Website/GitHub/hnest/node_modules/.store/update@0.7.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/Users/david/Documents/Work/Website/GitHub/hnest/node_modules/.store/update@0.7.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/update@0.7.4/node_modules/update/bin/update.js" "$@"
else
  exec node  "$basedir/../.store/update@0.7.4/node_modules/update/bin/update.js" "$@"
fi
